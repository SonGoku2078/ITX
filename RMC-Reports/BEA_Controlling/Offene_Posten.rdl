    <DataSet Name="DSet_Par_Monat_OpFact">
      <Query>
        <DataSourceName>DS_DWH_SHARED</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@ds_param_jahr">
            <Value>=Parameters!ParJahr.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>-- 2018-09-05 : DueDate durch JahrMonat ersetzt, weil nur der StichMonat angezeit wurde nicht jedoch der tatsächlich abgeschlossene Monat
-- 2018-07-18 : "Name" in Datum gewandelt um Abfrage eingebaut, so dass nur Daten ab dem April 2018 angezeigt werden
use DWH;
SELECT distinct
 CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,1,4) AS INT) AS Jahr
,CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) AS MonatNo
,CASE
    WHEN CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) =  1 THEN 'Januar'
    WHEN CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) =  2 THEN 'Februar'
    WHEN CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) =  3 THEN 'März'
    WHEN CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) =  4 THEN 'April'
    WHEN CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) =  5 THEN 'Mai'
    WHEN CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) =  6 THEN 'Juni'
    WHEN CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) =  7 THEN 'Juli'
    WHEN CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) =  8 THEN 'August'
    WHEN CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) =  9 THEN 'September'
    WHEN CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) = 10 THEN 'Oktober'
    WHEN CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) = 11 THEN 'November'
    WHEN CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,6,2) AS INT) = 12 THEN 'Dezember'
    ELSE Concat('Unbekannte Monatsnummer =&gt;', ' "', CAST(SUBSTRING(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat,5,2) AS INT), '"')
 END AS MonatTxtLong
,	   [JahrMonat]as Buchungsmonat
,	   [DueDate]  as StichtagTimestamp
,Year ([DueDate]) as StichtagJahr
,Month([DueDate]) as StichtagMonat
,Day  ([DueDate]) as StichtagTag

FROM rmc.FACT_OFFENE_POSTEN_HIST
WHERE 
	(IstFreigegeben is not null) AND 
	CAST(LEFT(rmc.FACT_OFFENE_POSTEN_HIST.JahrMonat, 4) AS INT) = @ds_param_jahr

ORDER BY jahr desc, MonatNo DESC</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="Jahr">
          <DataField>Jahr</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MonatNo">
          <DataField>MonatNo</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="MonatTxtLong">
