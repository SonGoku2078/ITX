            <Query>
              <Statement>// 13.06.2018 : Kennzahlen "Anzahl" &amp; "Summe" für jeweils "Total"- und "überfällige" Offene Posten angelegt
//

EVALUATE	
ADDCOLUMNS(
	SUMMARIZECOLUMNS(
		 'OffenePostenDatum'[Jahr]
		,'OffenePostenDatum'[Jahr-Monat]
		,'OffenePostenDatum'[Monatsname]
		,'OffenePostenDatum'[Monatsnummer]
	    ,FILTER (
	    		VALUES('OffenePostenDatum'[Datum]),
		       	(  
//			       	'OffenePostenDatum'[Datum] &lt;= DATE(@ds_param_jahr ,@ds_param_monat    , 31) &amp;&amp;
//			       	'OffenePostenDatum'[Datum] &gt;= DATE(@ds_param_jahr ,@ds_param_monat -23,  1)
			       	'OffenePostenDatum'[Datum] &lt;= DATE(2018 ,6    , 31) &amp;&amp;
			       	'OffenePostenDatum'[Datum] &gt;= DATE(2018 ,6 -23,  1)   			       	  
		       	)
		       	) 
		,"OP Total - Anzahl"		,[Anzahl Offene Posten Total]
		,"OP überfällig - Anzahl"	,[Anzahl Offene Posten Fällig]
		,"OP Total - Summe"			,ABS(ROUND([Summe Offene Posten Total]	/ 1000,2))
		,"OP überfällig - Summe"	,ABS(ROUND([Summe Offene Posten Fällig]	/ 1000,2))
		
		
	) //Summarized
	
//	,"AJahr"		, 'OffenePostenDatum'[Jahr]	
	,"Monat_Short"	, LEFT('OffenePostenDatum'[Monatsname],3)
//	,"Jahr_Monat"	, 'OffenePostenDatum'[Jahr] &amp; "-" &amp; 'OffenePostenDatum'[Monatsnummer]
	,"SortDate" 	, DATE('OffenePostenDatum'[Jahr],'OffenePostenDatum'[Monatsnummer],01)
) // ADDColumn				

Order by [Jahr-Monat] desc
</Statement>
              <ParameterDefinitions />
            </Query>
