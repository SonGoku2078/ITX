        <Field Name="c_mod_Column">
          <Value>= IIF(Fields!Index.Value mod 4 = 0,4,Fields!Index.Value mod 4)</Value>
        </Field>
        <Field Name="c_mod_Row">
          <Value>= ceiling(Fields!Index.Value /4)</Value>
