  <DataSets>
    <DataSet Name="DSet_KundenUmsatz_Small_Multiples">
      <Query>
        <DataSourceName>DS_RMC_Tabular_SHARED</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="ds_param_debitor">
            <Value>=Parameters!ParDebitor.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
          <QueryParameter Name="ds_param_jahr">
            <Value>=Parameters!ParJahr.Value</Value>
            <rd:UserDefined>true</rd:UserDefined>
          </QueryParameter>
        </QueryParameters>
        <CommandText>// 13.06.2018 : Create Query
//

EVALUATE
ADDCOLUMNS(
	SUMMARIZECOLUMNS (
	
	    'MaterialFaktura'[Einsatzmittel],
	    'Fakturadatum'[CAL_YEAR],
	    'Kunde'[Assistance],
		FILTER (
		   		VALUES('Fakturadatum'[CAL_YEAR]),
		      	(  
		       	'Fakturadatum'[CAL_YEAR] &lt;= @ds_param_jahr 	&amp;&amp;
		       	'Fakturadatum'[CAL_YEAR] &gt;= @ds_param_jahr -5     
				)
		       	),   
		FILTER (
		   		VALUES('Kunde'[Assistance]),
		      	(  
		       	'Kunde'[Assistance] = @ds_param_debitor		      	
//		       	'Kunde'[Assistance] = "Mobi24" 
				)
		       	),   


	    "Umsatz", [Umsatz] / 1000
	)
	,"Index", RANKX (ALL('MaterialFaktura'[Einsatzmittel])
					,CALCULATE(DISTINCT('MaterialFaktura'[Einsatzmittel]))
					,
					,TRUE()
					) // Index
)
ORDER BY [Index] desc</CommandText>
        <rd:DesignerState>
          <QueryDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://schemas.microsoft.com/AnalysisServices/QueryDefinition">
            <CommandType>DAX</CommandType>
