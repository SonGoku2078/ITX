            <QuerySpecification xsi:type="MDXQuerySpecification">
              <Select>
                <Items>
                  <Item>
                    <ID xsi:type="Level">
                      <DimensionName>MaterialFaktura</DimensionName>
                      <HierarchyName>Einsatzmittel</HierarchyName>
                      <HierarchyUniqueName>[MaterialFaktura].[Einsatzmittel]</HierarchyUniqueName>
                      <LevelName>Einsatzmittel</LevelName>
                      <UniqueName>[MaterialFaktura].[Einsatzmittel].[Einsatzmittel]</UniqueName>
                    </ID>
                    <ItemCaption>Einsatzmittel</ItemCaption>
                  </Item>
                  <Item>
                    <ID xsi:type="Level">
                      <DimensionName>Kunde</DimensionName>
                      <HierarchyName>Assistance</HierarchyName>
                      <HierarchyUniqueName>[Kunde].[Assistance]</HierarchyUniqueName>
                      <LevelName>Assistance</LevelName>
                      <UniqueName>[Kunde].[Assistance].[Assistance]</UniqueName>
                    </ID>
                    <ItemCaption>Assistance</ItemCaption>
                  </Item>
                  <Item>
                    <ID xsi:type="Measure">
                      <MeasureName>Umsatz</MeasureName>
                      <UniqueName>[Measures].[Umsatz]</UniqueName>
                    </ID>
                    <ItemCaption>Umsatz</ItemCaption>
                  </Item>
                </Items>
              </Select>
              <AutoExecute>false</AutoExecute>
              <From>RMC-Tabular</From>
              <Filter>
                <FilterItems />
              </Filter>
              <Calculations />
              <Aggregates />
              <QueryProperties />
            </QuerySpecification>
            <Query>
              <Statement>// 13.06.2018 : Create Query
//
EVALUATE
SUMMARIZECOLUMNS (

    'MaterialFaktura'[Einsatzmittel],
//    'Fakturadatum'[CAL_YEAR],
    'Kunde'[Assistance],
	FILTER (
	   		VALUES('Fakturadatum'[CAL_YEAR]),
	      	(  
//	       	'Fakturadatum'[CAL_YEAR] &lt;= @ds_param_jahr 	&amp;&amp;
//	       	'Fakturadatum'[CAL_YEAR] &gt;= @ds_param_jahr -5     
	       	'Fakturadatum'[CAL_YEAR] &lt;= 2018 			&amp;&amp;
	       	'Fakturadatum'[CAL_YEAR] &gt;= 2018 -5
			)
	       	),     
    "Umsatz", [Umsatz]
)
ORDER BY 1 desc, 2 desc</Statement>
              <ParameterDefinitions />
            </Query>
          </QueryDefinition>
        </rd:DesignerState>
      </Query>
