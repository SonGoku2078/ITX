<?xml version="1.0" encoding="utf-8"?>
<SharedDataSet xmlns="http://schemas.microsoft.com/sqlserver/reporting/2010/01/shareddatasetdefinition" xmlns:rd="http://schemas.microsoft.com/SQLServer/reporting/reportdesigner">
  <DataSet Name="DataSet1">
    <Query>
      <DataSourceReference>DS_TABULAR</DataSourceReference>
      <DataSetParameters>
        <DataSetParameter Name="DS_Param_Basistyp">
          <DefaultValue>="Mittelland"</DefaultValue>
          <ReadOnly>false</ReadOnly>
          <Nullable>false</Nullable>
          <OmitFromQuery>false</OmitFromQuery>
          <rd:UserDefined>true</rd:UserDefined>
          <rd:IsMultiValued>true</rd:IsMultiValued>
        </DataSetParameter>
      </DataSetParameters>
      <CommandText>DEFINE
	VAR 
		DS_Variable_Basistyp = @DS_Param_Basistyp
//		DS_Variable_Basistyp = "Mittelland"
 

EVALUATE

SUMMARIZECOLUMNS (
    'Basis'[Basis],
    'Basis'[Basistyp],
    FILTER (
        VALUES ( 'Basis'[Basis] ),
        ( 'Basis'[Basis] = "AGLL" )
            || ( 'Basis'[Basis] = "AGLS" )
            || ( 'Basis'[Basis] = "EBBE" )
            || ( 'Basis'[Basis] = "EBBO" )
            || ( 'Basis'[Basis] = "EBBS" )
            || ( 'Basis'[Basis] = "EBEH" )
            || ( 'Basis'[Basis] = "EBEN" )
            || ( 'Basis'[Basis] = "EBGE" )
            || ( 'Basis'[Basis] = "EBGR" )
            || ( 'Basis'[Basis] = "EBLS" )
            || ( 'Basis'[Basis] = "EBMO" )
            || ( 'Basis'[Basis] = "EBSG" )
            || ( 'Basis'[Basis] = "EBTI" )
            || ( 'Basis'[Basis] = "EBTU" )
            || ( 'Basis'[Basis] = "EBUR" )
            || ( 'Basis'[Basis] = "EBZH" )
            || ( 'Basis'[Basis] = "EBZW" )
            || ( 'Basis'[Basis] = "HBES" )
    ),
    FILTER (
        VALUES ( 'Basis'[Basistyp] ),
        	   ( 'Basis'[Basistyp] = DS_Variable_Basistyp ) 
    )
)</CommandText>
