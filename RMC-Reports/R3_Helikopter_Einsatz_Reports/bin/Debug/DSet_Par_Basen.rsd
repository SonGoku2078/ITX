            <Statement>EVALUATE

SUMMARIZECOLUMNS (
    'Basis'[Basis],
    'Basis'[Basistyp],
    FILTER (
        VALUES ( 'Basis'[Basis] ),
        ( 'Basis'[Basis] = "AGLL" )
            || ( 'Basis'[Basis] = "AGLS" )
            || ( 'Basis'[Basis] = "EBBE" )
            || ( 'Basis'[Basis] = "EBBO" )
            || ( 'Basis'[Basis] = "EBBS" )
